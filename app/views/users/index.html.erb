<h1>League Users</h1>



<table class="table table-hover">

  <thead>
  <tbody>
    <tr class="table-active">
      <th scope="row">First Name</th>
      <td>Last Name</td>
      <td>Username</td>


      </tbody>


    </tr>
    <% @users.each do |user| %>

    <tr>

      <th scope="col"><%= "#{user.first_name}" %></th>
      <th scope="col"><%= "#{user.last_name}"  %></th>
      <th scope="col"><%= link_to "#{user.username}", user_path(user)  %></th>
     

      <% end %>

    </tr>

  </thead>
 
</table> 


