<%= render 'partial' %>



<center>
    <% if @player.team.name == "Free Agent" %>
    <%= button_to  "Add Player", edit_player_path, method: "get", class: "btn btn-outline-success"  %>
    <% end %>
    
    <% if @current_user.teams.include?(@player.team) %>
    <%= button_to  "Release Player", release_path(@player), method: "get", class: "btn btn-outline-success"  %>
    <% end %>
    </center>

    